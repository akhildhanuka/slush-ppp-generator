{
  "name": "<%= appName %>",
  "version": "<%= appVersion %>",
  "description": "<%= appDescription %>",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "install-globals": "npm install --global cross-env http-server",
    "build": "cross-env UV_THREADPOOL_SIZE=128 webpack --progress",
    "build-unminified": "cross-env UV_THREADPOOL_SIZE=128 UNMINIFIED=true webpack --progress",
    "watch": "cross-env UV_THREADPOOL_SIZE=128 webpack --watch",
    "watch-unminified": "cross-env UV_THREADPOOL_SIZE=128 UNMINIFIED=true webpack --watch --progress",
    "release": "cross-env UV_THREADPOOL_SIZE=128 MODE=release webpack --progress",
    "release-live": "cross-env UV_THREADPOOL_SIZE=128 MODE=release PUBLIC_PATH='//west.cdn.mathletics.com/html/login/' webpack --progress",
    "ci": "cross-env UV_THREADPOOL_SIZE=128 NAMES=release* MODE=release webpack"
  },
  "author": "<%= authorName %>",
  "license": "ISC",
  "dependencies": {
    "adjust-sourcemap-loader": "~1.0.0",
    "angular": "1.5.3",
    "angular-ui-router": "0.2.18",
    "angular-mocks": "~1.5.3",
    "angular-sanitize": "1.3.15",
    "api-check": "7.5.5",
    "babel-core": "~6.4.5",
    "babel-loader": "~6.2.1",
    "babel-plugin-add-module-exports": "0.1.1",
    "babel-preset-es2015": "6.3.13",
    "babel-preset-es2015-loose": "7.0.0",
    "bower-webpack-plugin": "~0.1.9",
    "browser-sync": "~2.11.1",
    "browser-sync-webpack-plugin": "~1.0.1",
    "camelcase": "~2.1.0",
    "chunk-manifest-webpack-plugin": "0.0.1",
    "clean-webpack-plugin": "~0.1.7",
    "cross-env": "3.1.3",
    "css-loader": "~0.23.1",
    "entry-generator-webpack-plugin": "~0.2.1",
    "esmangle-webpack-plugin": "~0.0.1",
    "exports-loader": "~0.6.2",
    "extract-text-webpack-plugin": "~1.0.1",
    "file-loader": "~0.8.5",
    "glob": "~6.0.4",
    "html-loader": "~0.4.0",
    "html-webpack-plugin": "^2.15.0",
    "image-webpack-loader": "~1.6.3",
    "jshint": "~2.9.1",
    "jshint-loader": "~0.8.3",
    "json-loader": "~0.5.4",
    "jquery": "2.1.4",
    "lodash.assign": "^4.0.7",
    "md5": "~2.1.0",
    "ng-annotate-loader": "~0.1.0",
    "ng-focus-if": "1.0.2",
    "nginject-loader": "~2.1.0",
    "node-sass": "~3.4.2",
    "omit-tilde-webpack-plugin": "~0.1.1",
    "resolve-url-loader": "~1.4.3",
    "sass-loader": "~3.1.2",
    "style-loader": "0.12.3",
    "url-loader": "~0.5.7",
    "webpack": "~1.12.12"
  },
  "optionalDependencies": {
    "fsevents": "1.0.0"
  }
}
